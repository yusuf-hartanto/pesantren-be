{
  "name": "poc-api",
  "version": "1.0.0",
  "description": "poc",
  "main": "server.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/server.js\"",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "db:migrate": "ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "db:migrate-undo": "ts-node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all",
    "make:seed": "npx sequelize-cli seed:generate --name",
    "db:seed": "ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "db:seed-undo": "ts-node ./node_modules/sequelize-cli/lib/sequelize db:seed:undo:all"
  },
  "sequelize": {
    "config": "config/config.ts",
    "seeders-path": "seeders"
  },
  "keywords": [],
  "author": "alpha cipta teknologi",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/sequelize": "^4.28.20",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.5.1",
    "express-xss-sanitizer": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.16",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "querystring": "^0.2.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sharp": "^0.33.5",
    "tele-sender": "^1.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "uuid": "^11.0.5",
    "uuidv4": "^6.2.13"
  }
}
